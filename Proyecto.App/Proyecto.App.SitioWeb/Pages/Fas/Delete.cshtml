@page
@model Proyecto.App.SitioWeb.Pages.Fas.DeleteModel
@{
}
<h1>Eliminar Fase</h1>
<form method='post'>
    <input type="hidden" asp-for="fase.faseId" />
    <h2>Esta seguro de borrar el registro de @Model.fase.nombreFase ?</h2>
    <h3>@Model.respuesta</h3>
    <input type="submit" value="Borrar" class="btn btn-danger" />
    <a asp-page="/Fas/List">Back to list</a>
</form>
